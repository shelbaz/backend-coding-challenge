{
  "name": "backend-coding-challenge",
  "version": "1.0.0",
  "description": "(inspired by https://github.com/busbud/coding-challenge-backend-c)",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "start": "node src/build/app.js",
    "test": "export NODE_ENV=test &&  sequelize db:migrate:undo:all  && sequelize db:migrate  && nyc --require @babel/register  mocha ./test/test.js --timeout 20000 --exit",
    "build": "rm -rf ./src/build && babel -d ./src/build ./src -s",
    "dbcreate": "createdb cities && sequelize db:migrate",
    "createdb": "createdb",
    "sequelize": "sequelize",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shelbaz/backend-coding-challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shelbaz/backend-coding-challenge/issues"
  },
  "homepage": "https://github.com/shelbaz/backend-coding-challenge#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/cli": "^7.5.5",
    "@babel/runtime": "^7.4.3",
    "babel-cli": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.6",
    "chai": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.0"
  },
  "dependencies": {
    "babel-core": "^7.0.0-beta.44",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^7.0.0-beta.44",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "csv": "^5.1.1",
    "ejs": "^2.6.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "logger": "0.0.1",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.12.3",
    "xml2js": "^0.4.19"
  }
}
